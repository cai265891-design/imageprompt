{
  "projectSettings": {
    "framework": null,
    "buildCommand": "bun run build",
    "outputDirectory": ".vercel/output",
    "rootDirectory": "apps/auth-proxy",
    "installCommand": "bun install",
    "devCommand": "bun run dev",
    "ignoreCommand": "git diff --quiet HEAD^ HEAD apps/auth-proxy/ packages/auth/ packages/db/"
  },
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/routes/[...auth].ts"
    }
  ]
}
